;; shadow-cljs configuration
{:source-paths ["src"]
 :dev-http
 {4041 {:root "resources/public"
        :proxy-url "http://localhost:4040"}}

 :builds
 {;; npx shadow-cljs watch app
  :app
  {:target :browser
   :output-dir "resources/public/js"
   :asset-path "/js"
   :devtools {:watch-dir "resources/public"}
   :modules {:admin ; becomes public/js/admin.js
             {:init-fn rtc.admin/init}}}}}
